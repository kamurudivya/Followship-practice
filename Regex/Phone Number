package com.bridgelabz.Regex;
import java.util.Scanner;

public class phoneNumber {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter your passwordvalidation: ");
        String phone = sc.next();
        String strPattern = "^[0-9]{10}$";
        boolean result = phone.matches(strPattern);
        if(result) {
            System.out.println("Given phonenumber is valid");
        } else {
            System.out.println("Given phonenumber is not valid");
        }
    }
}
